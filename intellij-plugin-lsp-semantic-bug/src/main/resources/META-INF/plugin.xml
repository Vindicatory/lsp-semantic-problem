<idea-plugin>
    <name>typealiasproblem</name>
    <vendor>Vindicatory</vendor>
    <depends>com.intellij.modules.ultimate</depends>

    <extensions defaultExtensionNs="com.intellij">
        <platform.lsp.serverSupportProvider
            implementation="typealiasproblem.sample.SampleServerSupportProvider" />

        <fileType
                name="Sample File"
                implementationClass="typealiasproblem.sample.SampleFileType"
                fieldName="INSTANCE"
                language="XML"
                extensions="testfiletype" />

    </extensions>
</idea-plugin>
